{
	"name": "guineabot-ts",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
		"lint": "prettier --write .",
		"build": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Cy1der/guineabot-ts.git"
	},
	"keywords": [],
	"author": "Cy1der",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Cy1der/guineabot-ts/issues"
	},
	"homepage": "https://github.com/Cy1der/guineabot-ts#readme",
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-angular": "^11.0.0",
		"@types/glob": "^7.1.3",
		"@types/ms": "^0.7.31",
		"@types/node": "^14.14.21",
		"@types/prettier": "^2.1.6",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.1.1",
		"typescript": "^4.1.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn dlx @yarnpkg/doctor && lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.ts": "prettier --write",
		"*.js": "prettier --write"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"dependencies": {
		"@discordjs/opus": "^0.3.3",
		"alpha-sort": "^4.0.0",
		"canvas": "^2.6.1",
		"consola": "^2.15.0",
		"discord-player": "^3.2.1",
		"discord-tictactoe": "^2.0.7",
		"discord.js": "^12.5.1",
		"discord.js-minesweeper": "^1.0.6",
		"glob": "^7.1.6",
		"got": "^11.8.1",
		"maze-generation": "^2.1.0",
		"ms": "^2.1.3",
		"node-opus": "^0.3.3",
		"opusscript": "^0.0.7",
		"quick.db": "^7.1.3",
		"sudoku": "^0.0.3",
		"ytdl-core": "latest"
	}
}
